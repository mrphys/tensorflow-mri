{
	"dockerFile": "./Dockerfile",
	"settings": {
		"terminal.integrated.defaultProfile.linux": "bash"
	},
	"extensions": [
		"ms-python.python",
		"ms-vscode.cpptools"
	],
	"runArgs": [
		"--gpus=all",
	],
	"postCreateCommand": "python3.8 -m pip install -r requirements.txt",
	"remoteUser": "vscode"
}
